     <HEAD>
      <TDOBJECT>DOKU</TDOBJECT>
      <TDNAME>ZATB_MAKTX</TDNAME>
      <TDID>DE</TDID>
      <TDSPRAS>D</TDSPRAS>
      <TDFORM>S_DOCU_SHOW</TDFORM>
      <TDSTYLE>S_DOCUS1</TDSTYLE>
      <TDVERSION>00003</TDVERSION>
      <TDLINESIZE>072</TDLINESIZE>
     </HEAD>
     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEFINITION&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Bis zu 40 Stellen langer Text, der das &lt;GL:Material&gt;Material&lt;/&gt; näher</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>bezeichnet. Der Materialkurztext ist die Bezeichnung des Materials.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;USE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Es kann für beliebig viele Sprachen jeweils genau einen Kurztext geben.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;WHAT_TO_DO&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Prinzipiell erfassen Sie den Materialkurztext auf dem Datenbild des</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>ersten Fachbereichs, für den Sie Daten erfassen, und zwar in der</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Sprache, in der Sie angemeldet sind. Auf dem Grunddatenbild ist der</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Materialkurztext überschreibbar.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;EXAMPLES&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEPENDENCIES&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
     </LINES>
    </item>
